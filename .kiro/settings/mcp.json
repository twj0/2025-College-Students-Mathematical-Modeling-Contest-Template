{
  "mcpServers": {
    "mcp-feedback-enhanced": {
      "command": "uvx",
      "args": ["mcp-feedback-enhanced@latest"],
      "timeout": 600,
      "autoApprove": ["interactive_feedback"]
    }
  },

    "mcp-wolframalpha": {
      "url": "https://mcp.higress.ai/mcp-wolframalpha/cmezc8gvi00b0a701ikjmw0wg/sse"
  },
    


    "mcp-context7": {
            "url": "https://mcp.higress.ai/mcp-context7/cmezc8gvi00b0a701ikjmw0wg/sse"
  },


    "arxiv-mcp-server": {
      "command": "uv",
      "args": [
          "tool",
          "run",
          "arxiv-mcp-server",
          "--storage-path", "/path/to/paper/storage"
      ]
  },

  "mcp-server-chart": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@antv/mcp-server-chart"
      ]
  },

  "mermaid": {
      "command": "npx",
      "args": ["-y", "@peng-shawn/mermaid-mcp-server"]
    }

}
